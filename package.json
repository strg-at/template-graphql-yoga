{
  "name": "@strg-at/template-graphql-yoga",
  "version": "0.1.0",
  "description": "A template for GraphQL server using GraphQL Yoga and TypeScript",
  "main": "./dist/index.js",
  "scripts": {
    "test": "NODE_ENV=test nyc mocha src/**/*.spec.ts",
    "compile": "tsc",
    "start": "npm run compile && node -r ./dist/utils/opentelemetry/tracer.js ./dist/index.js",
    "build:ts-types": "graphql-codegen",
    "build": "npm run build:ts-types && rm -rf dist && tsc",
    "build:gql-schema": "npm run build && node ./dist/utils/export-schema.js",
    "lint": "npm run build:gql-schema && eslint \"src/**/*.ts\" --ignore-pattern \"src/**/*.spec.ts\"",
    "lint:fix": "npm run build:gql-schema && eslint \"src/**/*.ts\" --ignore-pattern \"src/**/*.spec.ts\" --fix",
    "service": "node --max-old-space-size=1024 -r ./dist/utils/opentelemetry/tracer.js ./dist/index.js",
    "dev": "tsnd ./src/index.ts"
  },
  "dependencies": {
    "@envelop/graphql-modules": "9.1.1",
    "@graphql-tools/executor-http": "1.3.3",
    "@graphql-tools/schema": "10.0.23",
    "@graphql-tools/stitch": "9.4.22",
    "@graphql-yoga/plugin-prometheus": "6.13.0",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/core": "1.30.1",
    "@opentelemetry/exporter-jaeger": "1.30.1",
    "@opentelemetry/instrumentation-express": "0.48.1",
    "@opentelemetry/instrumentation-graphql": "0.48.0",
    "@opentelemetry/instrumentation-http": "0.200.0",
    "@opentelemetry/resources": "1.30.1",
    "@opentelemetry/sdk-trace-node": "1.30.1",
    "@opentelemetry/semantic-conventions": "1.32.0",
    "@strg/logging-winston": "3.0.1",
    "cors": "2.8.5",
    "express": "5.1.0",
    "express-prom-bundle": "8.0.0",
    "graphql-config": "5.1.5",
    "graphql-modules": "3.1.1",
    "graphql-scalars": "1.24.2",
    "graphql-tag": "2.12.6",
    "graphql-yoga": "5.18.0",
    "js-yaml": "4.1.1",
    "morgan": "1.10.1",
    "ts-custom-error": "3.3.1",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.5",
    "@graphql-codegen/typescript": "4.1.6",
    "@graphql-codegen/typescript-resolvers": "4.5.0",
    "@graphql-eslint/eslint-plugin": "4.4.0",
    "@stylistic/eslint-plugin": "4.2.0",
    "@types/chai": "5.2.1",
    "@types/chai-as-promised": "8.0.2",
    "@types/cors": "2.8.17",
    "@types/express": "5.0.1",
    "@types/js-yaml": "4.0.9",
    "@types/mocha": "10.0.10",
    "@types/morgan": "1.9.9",
    "@typescript-eslint/eslint-plugin": "8.31.1",
    "chai": "5.2.0",
    "chai-as-promised": "8.0.1",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-jsdoc": "50.6.11",
    "eslint-plugin-prettier": "5.2.6",
    "mocha": "11.1.0",
    "nyc": "17.1.0",
    "prettier": "3.5.3",
    "ts-mockito": "2.6.1",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "tsx": "4.19.4",
    "typescript": "5.7.3",
    "winston": "3.17.0"
  },
  "homepage": "https://github.com/strg-at/template-node#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strg-at/template-node.git"
  },
  "keywords": [],
  "author": "STRG.",
  "license": "SEE LICENSE IN LICENSE",
  "maintainers": []
}
